{
  "name": "nesity-statistics",
  "bugs": {
    "url": "https://github.com/niieani/nesity/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/niieani/nesity.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "type": "module",
  "keywords": [
    "statistics",
    "kde",
    "math",
    "kernel-density-estimate",
    "split-multimodal-distibution"
  ],
  "author": "Bazyli Brzoska <npm@invent.life> (https://invent.life)",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./esm/main.js",
      "require": "./cjs/main.js",
      "types": "./src/main.ts"
    },
    "./*": {
      "import": "./esm/*.js",
      "require": "./cjs/*.js",
      "types": "./src/*.ts"
    },
    "./cjs": {
      "require": "./cjs/main.js",
      "types": "./src/main.ts"
    },
    "./cjs/*": {
      "require": "./cjs/*.js",
      "types": "./src/*.ts"
    },
    "./esm/*": {
      "import": "./esm/*.js",
      "types": "./src/*.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "cjs/main.js",
  "module": "esm/main.js",
  "source": "src/main.ts",
  "files": [
    "cjs",
    "esm",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "rrun": "rrun",
    "create-cjs-stub-package": "echo '{\"type\": \"commonjs\"}' > cjs/package.json"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      {
        "name": "main",
        "channel": false
      },
      "next",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  },
  "dependencies": {
    "@stdlib/stats-kstest": "^0.1.0",
    "@stdlib/stats-ttest2": "^0.1.0",
    "jerzy": "^0.2.1",
    "jstat": "^1.9.6"
  },
  "devDependencies": {
    "@niieani/scaffold": "^1.7.11",
    "@types/asciichart": "^1.5.8",
    "asciichart": "^1.5.25"
  }
}
